{
  "name": "pawwwloff/sulu-article-bundle",
  "type": "sulu-bundle",
  "description": "Simple article Bundle for Sulu 2 CMS",
  "keywords": [
    "sulu",
    "suluCms",
    "article"
  ],
  "homepage": "https://github.com/pawwwloff/SuluArticleBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Evgeny Pavlov"
    }
  ],
  "require": {
    "php": "^8.1",
    "laminas/laminas-zendframework-bridge": "^1.4",
    "rector/rector": "^0.15.7",
    "sulu/sulu": "2.5.*",
    "sulu/sulu-rector": "^0.1.3",
    "symfony/config": "^4.4 || ^5.0 || ^6.0",
    "symfony/dependency-injection": "^4.4 || ^5.0 || ^6.0",
    "symfony/framework-bundle": "^4.4 || ^5.0 || ^6.0",
    "symfony/http-foundation": "^4.4 || ^5.0 || ^6.0",
    "symfony/http-kernel": "^4.4 || ^5.0 || ^6.0"
  },
  "require-dev": {
    "coduo/php-matcher": "^5.0 || ^6.0",
    "handcraftedinthealps/zendsearch": "^2.0",
    "jackalope/jackalope-doctrine-dbal": "^1.3.4",
    "phpspec/prophecy": "^1.15",
    "symfony/browser-kit": "^4.4 || ^5.0 || ^6.0",
    "symfony/console": "^4.4 || ^5.0 || ^6.0",
    "symfony/dotenv": "^4.4 || ^5.0 || ^6.0",
    "symfony/error-handler": "^4.4 || ^5.0 || ^6.0",
    "symfony/monolog-bundle": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "Pawwwloff\\Bundle\\SuluArticleBundle\\": ""
    }
  },
  "config": {
    "sort-packages": true
  }
}
